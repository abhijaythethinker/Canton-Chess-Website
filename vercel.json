{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/api.js",
      "use": "@vercel/node",
      "config": {
        "memory": 1024,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/get-membership-expiration",
      "dest": "/api/api.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}